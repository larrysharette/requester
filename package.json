{
  "name": "requester",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*{.js,.test.js}\"",
    "format:check": "prettier --list-different \"{src,test}/**/*{.js,.test.js}\"",
    "lint": "eslint src test",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "prepare": "npm run format:check && npm run lint && npm test",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larrysharette/requester.git"
  },
  "author": "Larry Sharette",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larrysharette/requester/issues"
  },
  "homepage": "https://github.com/larrysharette/requester#readme",
  "devDependencies": {
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "*",
    "@babel/cli": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-classes": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "regenerator-runtime": "^0.12.1"
  },
  "npmName": "requester",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "sideEffects": false
}
